{
  "name": "langgraph-search-system",
  "version": "1.0.0",
  "description": "Maximum Quality Search Architecture with LangGraph",
  "_moduleAliases": {
    "@": "dist",
    "@/config": "dist/config",
    "@/types": "dist/types",
    "@/services": "dist/services",
    "@/utils": "dist/utils"
  },
  "scripts": {
    "build": "tsc",
    "build:prod": "tsc --sourceMap false",
    "start": "node dist/server.js",
    "start:prod": "NODE_ENV=production node dist/server.js",
    "start:prod:pm2": "NODE_ENV=production pm2 start ecosystem.config.js --env production",
    "stop:prod": "pm2 stop search-api",
    "restart:prod": "pm2 restart search-api",
    "reload:prod": "pm2 reload search-api",
    "logs:prod": "pm2 logs search-api",
    "monit:prod": "pm2 monit",
    "deploy:prod": "npm run build:prod && npm run start:prod:pm2",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only src/server.ts",
    "dev:nodemon": "nodemon",
    "setup": "ts-node -r tsconfig-paths/register src/setup-axios.ts",
    "test": "jest",
    "test:e2e": "jest --config jest.e2e.config.js",
    "test:e2e:watch": "jest --config jest.e2e.config.js --watch",
    "test:security": "./test/security/manual-security-tests.sh",
    "test:load": "echo 'Install k6 or Artillery to run load tests. See test/load/ for configurations.'",
    "test:enhanced": "ts-node -r tsconfig-paths/register run-tests.ts",
    "test:enhanced:unit": "ts-node -r tsconfig-paths/register run-tests.ts --category=unit",
    "test:enhanced:integration": "ts-node -r tsconfig-paths/register run-tests.ts --category=integration",
    "test:enhanced:performance": "ts-node -r tsconfig-paths/register run-tests.ts --category=performance",
    "test:enhanced:e2e": "ts-node -r tsconfig-paths/register run-tests.ts --category=e2e",
    "test:enhanced:coverage": "ts-node -r tsconfig-paths/register run-tests.ts --coverage",
    "test:integration": "ts-node -r tsconfig-paths/register test-complete-integration.ts",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "create-collections": "ts-node -r tsconfig-paths/register src/create-collections.ts",
    "seed-vectors": "ts-node -r tsconfig-paths/register src/seed-vectors.ts --force",
    "seed-vectors:build": "npm run build && node dist/src/seed-vectors.js",
    "test:script:intent": "npx tsx src/debug-scripts/test-intent-extractor-only.ts",
    "test:script:plan": "npx tsx src/debug-scripts/test-query-planner-only.ts",
    "test:script:executor": "npx tsx src/debug-scripts/test-query-executor-only.ts",
    "test:script:pipeline": "npx tsx src/debug-scripts/test-refactored-pipeline.ts",
    "setup:cache-indexes": "npx tsx src/debug-scripts/setup-cache-indexes.ts",
    "test:cache": "npx tsx src/debug-scripts/test-cache-implementation.ts",
    "verify-users": "ts-node -r tsconfig-paths/register scripts/verify-and-cleanup-users.ts verify",
    "cleanup-users": "ts-node -r tsconfig-paths/register scripts/verify-and-cleanup-users.ts cleanup",
    "test:ci-integration": "./scripts/test-ci-integration.sh"
  },
  "dependencies": {
    "@clerk/express": "^1.7.0",
    "@langchain/community": "^0.3.58",
    "@langchain/core": "^0.3.78",
    "@langchain/langgraph": "^0.2.19",
    "@langchain/ollama": "^0.2.4",
    "@langchain/openai": "^0.6.14",
    "@qdrant/js-client-rest": "^1.15.1",
    "@types/express": "^4.17.23",
    "@types/uuid": "^9.0.7",
    "axios": "^1.12.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^8.1.0",
    "express-validator": "^7.3.0",
    "fuse.js": "^7.0.0",
    "helmet": "^8.1.0",
    "hpp": "^0.2.3",
    "joi": "^18.0.1",
    "module-alias": "^2.2.3",
    "mongodb": "^6.17.0",
    "mongoose": "^8.20.1",
    "ollama": "^0.5.9",
    "opossum": "^9.0.0",
    "prom-client": "^15.1.3",
    "swagger-ui-express": "^5.0.1",
    "together-ai": "^0.27.0",
    "uuid": "^9.0.1",
    "winston": "^3.18.3",
    "winston-loggly-bulk": "^3.3.2",
    "yamljs": "^0.3.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@faker-js/faker": "^9.4.0",
    "@testcontainers/qdrant": "^10.18.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.9.1",
    "@types/nodemon": "^1.19.6",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "eslint": "^9.15.0",
    "globals": "^16.5.0",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^10.1.3",
    "nodemon": "^3.1.10",
    "pm2": "^6.0.13",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.47.0"
  },
  "overrides": {
    "axios": "^1.12.2",
    "pm2": "^6.0.13",
    "js-yaml": "^4.1.1",
    "jws": "^3.2.3"
  },
  "volta": {
    "node": "24.10.0"
  }
}
