{
  "name": "langgraph-search-system",
  "version": "1.0.0",
  "type": "module",
  "description": "Maximum Quality Search Architecture with LangGraph",
  "scripts": {
    "build": "tsc",
    "build:prod": "tsc --sourceMap false",
    "start": "node dist/server.js",
    "start:prod": "NODE_ENV=production node dist/server.js",
    "start:prod:pm2": "NODE_ENV=production pm2 start ecosystem.config.js --env production",
    "stop:prod": "pm2 stop search-api",
    "restart:prod": "pm2 restart search-api",
    "reload:prod": "pm2 reload search-api",
    "logs:prod": "pm2 logs search-api",
    "monit:prod": "pm2 monit",
    "deploy:prod": "npm run build:prod && npm run start:prod:pm2",
    "dev": "tsx watch src/server.ts",
    "dev:nodemon": "nodemon",
    "setup": "tsx src/setup-axios.ts",
    "test": "jest",
    "test:e2e": "jest --config jest.e2e.config.js",
    "test:e2e:watch": "jest --config jest.e2e.config.js --watch",
    "test:security": "./test/security/manual-security-tests.sh",
    "test:load": "echo 'Install k6 or Artillery to run load tests. See test/load/ for configurations.'",
    "test:enhanced": "tsx run-tests.ts",
    "test:enhanced:unit": "tsx run-tests.ts --category=unit",
    "test:enhanced:integration": "tsx run-tests.ts --category=integration",
    "test:enhanced:performance": "tsx run-tests.ts --category=performance",
    "test:enhanced:e2e": "tsx run-tests.ts --category=e2e",
    "test:enhanced:coverage": "tsx run-tests.ts --coverage",
    "test:integration": "tsx test-complete-integration.ts",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "create-collections": "tsx src/create-collections.ts",
    "seed-vectors": "tsx src/seed-vectors.ts --force",
    "seed-vectors:build": "npm run build && node dist/src/seed-vectors.js",
    "test:script:intent": "npx tsx src/debug-scripts/test-intent-extractor-only.ts",
    "test:script:plan": "npx tsx src/debug-scripts/test-query-planner-only.ts",
    "test:script:executor": "npx tsx src/debug-scripts/test-query-executor-only.ts",
    "test:script:pipeline": "npx tsx src/debug-scripts/test-refactored-pipeline.ts",
    "setup:cache-indexes": "npx tsx src/debug-scripts/setup-cache-indexes.ts",
    "test:cache": "npx tsx src/debug-scripts/test-cache-implementation.ts",
    "verify-users": "tsx scripts/verify-and-cleanup-users.ts verify",
    "cleanup-users": "tsx scripts/verify-and-cleanup-users.ts cleanup",
    "test:ci-integration": "./scripts/test-ci-integration.sh"
  },
  "dependencies": {
    "@clerk/express": "^1.7.55",
    "@langchain/community": "^0.3.58",
    "@langchain/core": "^0.3.79",
    "@langchain/langgraph": "^0.2.74",
    "@langchain/ollama": "^0.2.4",
    "@langchain/openai": "^0.6.16",
    "@qdrant/js-client-rest": "^1.15.1",
    "@types/express": "^4.17.25",
    "@types/uuid": "^9.0.8",
    "axios": "^1.12.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^8.1.0",
    "express-validator": "^7.3.1",
    "fuse.js": "^7.0.0",
    "helmet": "^8.1.0",
    "hpp": "^0.2.3",
    "joi": "^18.0.2",
    "mongodb": "^6.17.0",
    "mongoose": "^8.20.2",
    "ollama": "^0.5.18",
    "opossum": "^9.0.0",
    "prom-client": "^15.1.3",
    "swagger-ui-express": "^5.0.1",
    "together-ai": "^0.27.0",
    "uuid": "^9.0.1",
    "winston": "^3.18.3",
    "winston-loggly-bulk": "^3.3.3",
    "yamljs": "^0.3.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@faker-js/faker": "^9.4.0",
    "@testcontainers/qdrant": "^10.18.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.9.4",
    "@types/nodemon": "^1.19.6",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "eslint": "^9.15.0",
    "globals": "^16.5.0",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^10.1.4",
    "nodemon": "^3.1.11",
    "pm2": "^6.0.14",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.6",
    "tsx": "^4.19.4",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.47.0"
  },
  "overrides": {
    "axios": "^1.12.2",
    "pm2": "^6.0.14",
    "js-yaml": "^4.1.1",
    "jws": "^3.2.3"
  },
  "volta": {
    "node": "24.10.0"
  },
  "imports": {
    "#config/*": "./dist/config/*.js",
    "#types/*": "./dist/types/*.js",
    "#services/*": "./dist/services/*.js",
    "#utils/*": "./dist/utils/*.js",
    "#middleware/*": "./dist/middleware/*.js",
    "#models/*": "./dist/models/*.js",
    "#routes/*": "./dist/routes/*.js",
    "#graphs/*": "./dist/graphs/*.js",
    "#nodes/*": "./dist/nodes/*.js",
    "#core/*": "./dist/core/*.js",
    "#shared/*": "./dist/shared/*.js",
    "#domains/*": "./dist/domains/*.js"
  }
}
