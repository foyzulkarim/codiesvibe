{
  "name": "langgraph-search-system",
  "version": "1.0.0",
  "description": "Maximum Quality Search Architecture with LangGraph",
  "_moduleAliases": {
    "@": "dist",
    "@/config": "dist/config",
    "@/types": "dist/types",
    "@/services": "dist/services",
    "@/utils": "dist/utils"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only src/server.ts",
    "setup": "ts-node -r tsconfig-paths/register src/setup-axios.ts",
    "test": "jest",
    "test:integration": "ts-node -r tsconfig-paths/register test-complete-integration.ts",
    "lint": "eslint src/**/*.ts",
    "type-check": "tsc --noEmit",
    "seed-vectors": "ts-node -r tsconfig-paths/register src/seed-vectors.ts",
    "seed-vectors:build": "npm run build && node dist/src/seed-vectors.js",
    "test-search": "ts-node -r tsconfig-paths/register test-intelligent-search.ts",
    "create-enhanced-collections": "ts-node -r tsconfig-paths/register src/scripts/create-enhanced-collections.ts",
    "test-enhanced-schema": "ts-node -r tsconfig-paths/register debug-scripts/test-enhanced-schema.ts",
    "seed-enhanced-vectors": "ts-node -r tsconfig-paths/register src/seed-enhanced-vectors.ts",
    "test-enhanced-vector-quality": "ts-node -r tsconfig-paths/register debug-scripts/test-enhanced-vector-quality.ts",
    "validate-enhanced-integration": "ts-node -r tsconfig-paths/register debug-scripts/validate-enhanced-integration.ts",
    "test-deduplication-quality": "ts-node -r tsconfig-paths/register debug-scripts/test-deduplication-quality.ts",
    "test-multi-vector-search": "ts-node -r tsconfig-paths/register debug-scripts/test-multi-vector-search.ts",
    "test-ner-only": "ts-node -r tsconfig-paths/register debug-scripts/test-ner-only.ts",
    "test-transformers-integration": "ts-node -r tsconfig-paths/register debug-scripts/test-transformers-integration.ts",
    "test-zero-shot-intent": "ts-node -r tsconfig-paths/register debug-scripts/test-zero-shot-intent.ts",
    "test-enhanced-graph": "ts-node -r tsconfig-paths/register debug-scripts/test-enhanced-graph.ts",
    "test-dynamic-planning": "ts-node -r tsconfig-paths/register debug-scripts/test-dynamic-planning.ts"
  },
  "dependencies": {
    "@langchain/community": "^0.3.14",
    "@langchain/core": "^0.3.78",
    "@langchain/langgraph": "^0.2.19",
    "@langchain/ollama": "^0.2.4",
    "@langchain/openai": "^0.6.14",
    "@qdrant/js-client-rest": "^1.15.1",
    "@tensorflow/tfjs-node": "^4.22.0",
    "@types/express": "^4.17.23",
    "@types/uuid": "^9.0.7",
    "@xenova/transformers": "^2.17.2",
    "axios": "^1.12.2",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "fuse.js": "^7.0.0",
    "module-alias": "^2.2.3",
    "mongodb": "^6.17.0",
    "ollama": "^0.5.9",
    "onnxruntime-node": "^1.23.0",
    "uuid": "^9.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^22.9.1",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "eslint": "^9.15.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.3"
  }
}
