{
  "name": "langgraph-search-system",
  "version": "1.0.0",
  "description": "Maximum Quality Search Architecture with LangGraph",
  "_moduleAliases": {
    "@": "dist",
    "@/config": "dist/config",
    "@/types": "dist/types",
    "@/services": "dist/services",
    "@/utils": "dist/utils"
  },
  "scripts": {
    "build": "tsc",
    "build:prod": "tsc --sourceMap false",
    "start": "node dist/server.js",
    "start:prod": "NODE_ENV=production node dist/server.js",
    "start:prod:pm2": "NODE_ENV=production pm2 start ecosystem.config.js --env production",
    "stop:prod": "pm2 stop search-api",
    "restart:prod": "pm2 restart search-api",
    "reload:prod": "pm2 reload search-api",
    "logs:prod": "pm2 logs search-api",
    "monit:prod": "pm2 monit",
    "deploy:prod": "npm run build:prod && npm run start:prod:pm2",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only src/server.ts",
    "setup": "ts-node -r tsconfig-paths/register src/setup-axios.ts",
    "test": "jest",
    "test:enhanced": "ts-node -r tsconfig-paths/register run-tests.ts",
    "test:enhanced:unit": "ts-node -r tsconfig-paths/register run-tests.ts --category=unit",
    "test:enhanced:integration": "ts-node -r tsconfig-paths/register run-tests.ts --category=integration",
    "test:enhanced:performance": "ts-node -r tsconfig-paths/register run-tests.ts --category=performance",
    "test:enhanced:e2e": "ts-node -r tsconfig-paths/register run-tests.ts --category=e2e",
    "test:enhanced:coverage": "ts-node -r tsconfig-paths/register run-tests.ts --coverage",
    "test:integration": "ts-node -r tsconfig-paths/register test-complete-integration.ts",
    "lint": "eslint src/**/*.ts",
    "type-check": "tsc --noEmit",
    "create-collections": "ts-node -r tsconfig-paths/register src/create-collections.ts",
    "seed-vectors": "ts-node -r tsconfig-paths/register src/seed-vectors.ts --force",
    "seed-vectors:build": "npm run build && node dist/src/seed-vectors.js",
    "test:script:intent": "npx tsx src/debug-scripts/test-intent-extractor-only.ts",
    "test:script:plan": "npx tsx src/debug-scripts/test-query-planner-only.ts",
    "test:script:executor": "npx tsx src/debug-scripts/test-query-executor-only.ts",
    "test:script:pipeline": "npx tsx src/debug-scripts/test-refactored-pipeline.ts"
  },
  "dependencies": {
    "@langchain/community": "^0.3.57",
    "@langchain/core": "^0.3.78",
    "@langchain/langgraph": "^0.2.19",
    "@langchain/ollama": "^0.2.4",
    "@langchain/openai": "^0.6.14",
    "@qdrant/js-client-rest": "^1.15.1",
    "@tensorflow/tfjs-node": "^4.22.0",
    "@types/express": "^4.17.23",
    "@types/uuid": "^9.0.7",
    "@xenova/transformers": "^2.17.2",
    "axios": "^1.12.2",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "fuse.js": "^7.0.0",
    "module-alias": "^2.2.3",
    "mongodb": "^6.17.0",
    "ollama": "^0.5.9",
    "onnxruntime-node": "^1.23.0",
    "together-ai": "^0.27.0",
    "uuid": "^9.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^22.9.1",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "eslint": "^9.15.0",
    "jest": "^29.7.0",
    "pm2": "^5.3.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.3"
  },
  "volta": {
    "node": "24.10.0"
  }
}
