# Test Environment Configuration
NODE_ENV=test

# MongoDB Test Configuration
MONGODB_URI=mongodb://localhost:27017/test-codiesvibe
MONGODB_DB_NAME=test-codiesvibe

# Qdrant Test Configuration
QDRANT_HOST=localhost
QDRANT_PORT=6333
QDRANT_API_KEY=
QDRANT_COLLECTION_NAME=test-tools

# Application Configuration
PORT=4004
LOG_LEVEL=error
ENABLE_CACHE=false
CACHE_TTL=3600

# Security Configuration (disabled for tests)
ENABLE_VECTOR_VALIDATION=false
ENABLE_RATE_LIMITING=false
ENABLE_SECURITY_HEADERS=false

# Search Configuration
SEARCH_USE_MULTIVECTOR=true
MULTIVECTOR_MAX_RESULTS=20
VECTOR_TYPES=semantic,entities.categories,entities.functionality,entities.aliases,composites.toolType

# RRF configuration
SEARCH_RRF_K=60
SEARCH_SOURCE_WEIGHTS={"mongodb": 0.3, "qdrant": 0.7}
DEDUPE_THRESHOLD=0.8

# Score Threshold Configuration
SEARCH_SCORE_THRESHOLD=0.5
QUERY_EXECUTOR_SCORE_THRESHOLD=0.5
QUERY_EXECUTOR_HIGH_THRESHOLD=0.7

# Test API Keys (mocked)
TOGETHER_API_KEY=test-api-key-12345
TOGETHER_AI_API_KEY=test-api-key-12345
VLLM_BASE_URL=http://localhost:8000

# Logging Configuration (minimal for tests)
LOG_LEVEL=error
LOGGLY_ENABLED=false
LOG_FORMAT=json
